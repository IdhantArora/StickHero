<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane fx:id="mainPane" minHeight="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.HelloController">
   <children>
      <ImageView fx:id="IntroBg" fitHeight="400.0" fitWidth="600.0" layoutX="-1.0" pickOnBounds="true">
         <image>
            <Image url="@Images/IntroBg.jpg" />
         </image>
      </ImageView>
      <Rectangle fx:id="StartingBlock" arcHeight="5.0" arcWidth="5.0" height="117.0" layoutX="251.0" layoutY="283.0" stroke="BLACK" strokeType="INSIDE" width="114.0" />
      <ImageView fx:id="IntroBhai" fitHeight="56.0" fitWidth="43.0" layoutX="285.0" layoutY="240.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/StickBhai-removebgfinal.png" />
         </image>
      </ImageView>
      <ImageView fx:id="newGameButton" fitHeight="65.0" fitWidth="150.0" layoutX="233.0" layoutY="30.0" onMouseClicked="#startNewGame" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/NewGameButton1Bg.png" />
         </image>
      </ImageView>
      <ImageView fx:id="SavedGameButton" fitHeight="65.0" fitWidth="150.0" layoutX="233.0" layoutY="148.0" onMousePressed="#SavedGame" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/SavedGameButton1Bg.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
